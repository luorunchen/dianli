/*地图标题*/
.BMap_bubble_title {
  color: white;
  font-size: 13px;
  font-weight: bold;
  text-align: left;
  padding: 10px 0px 10px 10px;
  /* border-bottom: 1px solid gray; */
  background-color: #1A5F5D;
}

/* 消息内容 */
.BMap_bubble_content {
  background-image: linear-gradient(#082928, #103d38);
  padding: 10px 0px 10px 10px;
  color: #fff;
}

/* 内容 */
.BMap_pop div:nth-child(9) {
  top: 35px !important;
  border-radius: 7px;
  border:1px solid #15895D;
}

/* 左上角删除按键 */
.BMap_pop img {
  top: 43px !important;
  left: 215px !important;
  display: none;
}

.BMap_top {
  display: none;
}

.BMap_bottom {
  display: none;
}

.BMap_center {
  display: none;
}

/* 隐藏边角 */
.BMap_pop div:nth-child(1) div {
  display: none;
}

.BMap_pop div:nth-child(3) {
  display: none;
}

.BMap_pop div:nth-child(7) {
  display: none;
}

.BMap_pop div:nth-child(5) {
  display: none;
}