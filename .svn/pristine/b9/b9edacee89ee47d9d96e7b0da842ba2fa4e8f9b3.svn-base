.ax_default_index{
  justify-content: space-between;
}
.index_top{
  width:100%;
  height:202px;
  display: flex;
  justify-content: space-between; 
}
.index_left{
  width:75%;
  height:1050px;
  display: flex;
  justify-content: space-between;   
  flex-wrap: wrap;
  padding-top: 10px;
}
.index_left_top{
  width:31%;
  height:200px;
  text-align: left;
}
.index_left_top_head{
  width:100%;
  height:70px;
  color:#fff;
  line-height:70px;
}
.index_left_top_center{
  width:100%;
  height:100px;
  display: flex;
  justify-content: start;
}
.index_left_top_right{
  width:100%;
  height:30px;
  line-height:30px;
  text-align: left;
  background-color: #F9F9F9;
}


/******/
.index_center{
  width:100%;
  height:350px;
  display: flex;
  justify-content: space-between; 
}

.center_header{
 background: #FAFAFA;
 position: relative;
  text-align: left;
}
.center_all{
  text-align: left;    
  height: 260px;
  line-height: 30px;
  color:#757575;
}
#centet_map{
  height:300px;
}

/*******************/
.index_bottom{
  width:100%;
  height:400px;
  border:1px solid #D6D4D4;
}

/*************************/
.index_right{
  width:23%;
  height:1050px;
  border:1px solid #E0E0E0;
  margin-top:10px;
  overflow:hidden;
}
.right_all{
  overflow: auto;
  height:1050px;
}
