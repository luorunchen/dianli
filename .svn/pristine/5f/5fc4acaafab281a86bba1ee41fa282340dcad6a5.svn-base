.bodyAll {
  width: 260px;
  height: 740px;
  display: flex;
  align-content: start;
  flex-wrap: wrap;
  overflow: auto;
}

.bodyContent {
  width: 100%;
  height: 250px;
  border: 2px solid #249E77;
  margin: 10px 0 0 10px;
  position: relative;
}

.bodyDetail {
  height: 25px;
  line-height: 25px;
  width: 80px;
  background-image: linear-gradient(#1a5f52, #0b312b);
  position: absolute;
  right: 0px
}

.serviceData {
  width: 36.5%;
  border: 1px solid #159C61;
  background: rgba(6, 47, 43, 0.88);
  height: 242px;
  position: absolute;
  min-width: 450px;
}

.alarmRecord {
  width: 27.2%;
  border: 1px solid #159C61;
  background: rgba(6, 47, 43, 0.88);
  height: 268px;
  position: absolute;
  min-width: 250px;
}

.panalarm {
  width: 27.3%;
  border: 1px solid #159C61;
  background: rgba(6, 47, 43, 0.88);
  height:25%; 
  /* height:660px;   */
  position: absolute;
  min-width: 250px;
  z-index:99;
}
.alarmNum{
  float: right;
  color:red;
}

.legend {
  width: 130px;
  border: 1px solid #159C61;
  background: rgba(6, 47, 43, 0.88);
  height: 220px;
  position: absolute;
}

.imgPos {
  position: absolute;
  top: 10px;
  left: 15px;
}

.imgSpan {
  width: 50%;
  position: absolute;
  top: 15px;
  left: 50px;
}
.centerRound{
  width:100%;height:100px;background:url('../image/bigData.png');background-size:100% 100%;position: relative;
}
.serveRight{
  width:20px;height:30px;line-height:30px;line-height:30px;display:inline-block;    text-align: center;background-image: linear-gradient(#1bb193, #116053);
}
.serveBottom{
  width:20px;height:20px;display:inline-block;
  background:#1DC5A3
}
.serveBSpan{
  width:150px;height:20px;display:inline-block;position:absolute;margin-left: 5px;
}
.h25{
  height:25px;
}
.errorSpan{
  background:url('../image/error.png');  width: 15px;height: 15px;position: absolute;top: 11px;right: 10px;
}

/* leftList */
#panalUl::-webkit-scrollbar {
  /*滚动条整体样式*/
  width: 5px;
  /*高宽分别对应横竖滚动条的尺寸*/
  height: 5px;
}

#panalUl::-webkit-scrollbar-thumb {
  /*滚动条里面小方块*/
  border-radius: 5px;
  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
  background: #094D44;
}

#panalUl::-webkit-scrollbar-track {
  /*滚动条里面轨道*/
  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
  border-radius: 5px;
  background: #062F2B;
}