@media screen and (min-width: 1900px) {
  .bodyLeft {
    width: 18%;
    height: 100%;
    border: 1px solid #118153;
    position: relative;
    background: #062F2B;
  }
  
  .bodyRight {
    width: 80%;
    height: 100%;
    border: 1px solid #118153;
    position: relative;
    background: #062F2B;
    color: #fff;
  }
  
  .bodyRightTop {
    width: 98%;
    height: 30px;
    display: flex;
    justify-content: space-between;
    line-height: 30px;
    padding: 10px;
    border-bottom: 2px solid #08382A;
  }
  
  .bodyRightTopInput {
    height: 20px;
    outline: none;
    border: none;
    background: #0F4037;
    width: 90%;
    color: #fff;
    padding: 6px 12px;
  }
  
  .bodyRightAll {
    width: 100%;
    display: flex;
    justify-content: start;
    flex-wrap: wrap;
  }
  
  .bodyLeftHeader {
    width: 100%;
    height: 50px;
    line-height: 50px;
    text-align: center;
  }
  
  .bodyLeftInput {
    height: 20px;
    outline: none;
    border: none;
    background: #0F4037;
    width: 80%;
    color: #fff;
    padding: 6px 12px;
  }
  
  .bodyRightCenter {
    width: 23.5%;
    height: 230px;
    border: 2px solid #249E77;
    margin: 10px 0 0 10px;
  }
  
  .bodyRightCenterHeader {
    width: 100%;
    height: 50px;
    line-height: 50px;
    background: #0F4037;
    text-align: center;
  }
  
  .bodyRightCenterTitle {
    display: inline-block;
    line-height: 25px;
    height: 50px;
    margin: 30px 0 0 20px;
    font-size: 14px;
  }
  
  .bodyRightCenterDetail {
    height: 80px;
    display: flex;
    justify-content: center;
    text-align: center;
    position: relative;
  }
}
