<script>
  $('.bhBottom').click(function () {
    console.log($(this).attr('data-ad'))
    var ad = $(this).attr('data-ad')
    if (ad == '0') {
      location.href = "index.html"
    } else if (ad == '1') {
      location.href = "real_time.html"
    } else if (ad == '2') {
      location.href = "waringMessage.html"
    } else if (ad == '3') {
      location.href = "bigData.html"
    } else if (ad == '4') {
      // location.href = "bigData.html"
    } else {
      location.href = ""
    }
  })
